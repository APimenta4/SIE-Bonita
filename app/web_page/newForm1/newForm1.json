      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.etiqueta ){",
        "\treturn {",
        "\t\t//map etiqueta variable to expected task contract input",
        "\t\tetiquetaInput: {",
        "\t\t\ttipo: $data.etiqueta.tipo !== undefined ? $data.etiqueta.tipo : null,",
        "\t\t\tnomeCliente: $data.etiqueta.nomeCliente !== undefined ? $data.etiqueta.nomeCliente : null,",
        "\t\t\tnumeroIncidente: $data.etiqueta.numeroIncidente !== undefined ? $data.etiqueta.numeroIncidente : null,",
        "\t\t\tidentificacaoProduto: $data.etiqueta.identificacaoProduto !== undefined ? $data.etiqueta.identificacaoProduto : null,",
        "\t\t\tcodigoProduto: $data.etiqueta.codigoProduto !== undefined ? $data.etiqueta.codigoProduto : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "context" : {
