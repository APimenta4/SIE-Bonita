      "value" : [
        "if( $data.pedidoServico && $data.pedidoServico_pedidoSuporte ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.pedidoServico.pedidoSuporte = $data.pedidoServico_pedidoSuporte;",
        "\treturn {",
        "\t\t//map pedidoServico variable to expected task contract input",
        "\t\tpedidoServicoInput: {",
        "\t\t\tassinado: $data.pedidoServico.assinado !== undefined ? $data.pedidoServico.assinado : null",
